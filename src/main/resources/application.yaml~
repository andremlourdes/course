server:
  port: 8081

spring:
  application:
    name: authuser

  datasource:
    url: jdbc:postgresql://localhost:5433/ead-course
    username: postgres
    password: root
  jpa:
    properties:
      hibernate:
        dialect: org.hibernate.dialect.PostgreSQLDialect
    hibernate:
      ddl-auto: update
    show-sql: true
  logging:
    level:
      org:
        springframework:
          boot=DEBUG: DEBUG 